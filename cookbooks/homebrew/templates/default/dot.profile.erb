PATH="<%= @home %>/Developer/bin:$PATH"; export PATH
MANPATH="<%= @home %>/Developer/share/man:$MANPATH"; export MANPATH

export rvm_path=<%= @home %>/Developer/.rvm
if [[ -s <%= @home %>/Developer/.rvm/scripts/rvm ]] ; then
  source <%= @home %>/Developer/.rvm/scripts/rvm
fi

if [[ -f <%= @home %>/.cider.profile.custom ]]; then
  source ~/.cider.profile.custom
fi
