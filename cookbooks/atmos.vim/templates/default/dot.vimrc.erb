source ~/.vim/vimrc

let g:NERDTreeMapOpenSplit = 'i'
set nu
set et
set ts=2
set sw=2
colorscheme vibrantink
let g:fuzzy_ignore = "gems/*:coverage"
"set columns=145
"set lines=48
set guioptions-=T
set guioptions-=r
set guioptions-=L
set gfn=Monaco:h12
set backupdir=~/.vimswap
set directory=~/.vimswap
set nobackup
set noswapfile
"set maxwinheight=3
set winheight=11
command! Trail :%s/\s\+$//g
nnoremap <D-r> :NERDTreeToggle<CR>
set vb
set t_vb=
let g:loaded_syntastic_plugin = 1
